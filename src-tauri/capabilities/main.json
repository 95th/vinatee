{
  "identifier": "main",
  "description": "Main permissions",
  "local": true,
  "windows": [
    "main"
  ],
  "permissions": [
    "app:default",
    "dialog:allow-open",
    "dialog:allow-save",
    "dialog:default",
    "event:default",
    {
      "identifier": "http:default",
      "allow": [{ "url": "https://*" }, { "url" :"http://*"} ]
    },
    "menu:default",
    "path:default",
    "resources:default",
    "shell:allow-open",
    "shell:default",
    "tray:default",
    "window:default",
    "fs:default",
    "fs:allow-read"
  ]
}